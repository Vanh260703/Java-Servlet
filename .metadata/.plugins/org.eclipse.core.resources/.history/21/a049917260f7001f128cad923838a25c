<body>
	<div id = "myDiv" onmouseover = "changeDiv()">
		MY DIV
	</div>
	<script type="text/javascript">
		function changeDiv(){
			const myDiv = document.getElementById("myDiv");
			
			myDiv.style.color = "red";
		}
	</script>
</body>
